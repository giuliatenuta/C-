#include <iostream>
using namespace std;

void inserisci_valori(int &n1, int &n2){
	cout << "Valore numero 1: ";
	cin >> n1;
	cout << "Valore numero 2: ";
	cin >> n2;
}

int main(){
	int num1, num2;
	inserisci_valori(num1, num2);
	if ((num2 % num1) == 0) {
		cout << num2 << " è multiplo di " << num1;
	} else {
		cout << num2 << " non è multiplo di " << num1;
	}
	return 0;
}
